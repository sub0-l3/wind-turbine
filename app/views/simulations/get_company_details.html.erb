<div id="content">
	Select Company:
	<select id='company_name' >
		<option value=""></option>
		<% @company.each do |x|%>
		<option value="<%= x.id %>"><%= x.company_name %></option>
		<% end %>
	</select>
	Select Turbine: <select id = 'turbines_dropdown'></select>
	<br />
	<br />

	<div class = 'right'>
		<div id="turbine_details"></div>

	</div>
	<div class="demo-container left">
		<div>
			<i>Power Output(KW)</i>
		</div>
		<div id="placeholder" class="demo-placeholder"></div>
		<div class=right>
			<i>Wind Speed(m/s)</i>
		</div>
	</div>

	<!--
	<div class="demo-container">
	<div id="placeholder_empirical" class="demo-placeholder"></div>
	</div>
	-->

	<br />
	<div id='map-wrap'>
		<div class = 'right'>
			<div id="turbine_form"></div>
			<br />
			<div id="capacity">
				Capacity= <span id="num">0</span>%
			</div>

			<div class="demo-container-weibull"  style="display: none;">
				<div class="" style="margin-left: 60px">
					<small><i>Weibull curve</i></small>
				</div>
				<div id="placeholder-weibull" class="demo-placeholder"></div>
			</div>
		</div>

		<div id="map" class='left'></div>
	</div>
